#include <stdio.h>
int main() 
{
int , original, remainder, reversed=0;
    printf("Enter a number:");
    scanf("%d",&x);
    original=x;
    
    while(x!=0){
        remainder=x%10;
        reversed=reversed*10+remainder;
        x=x/10;
    }
    
    if(original==reversed)
        printf("The given number is a palindrome");
    else
        printf("The given number is not a palindrome");
    
    return 0;
}
